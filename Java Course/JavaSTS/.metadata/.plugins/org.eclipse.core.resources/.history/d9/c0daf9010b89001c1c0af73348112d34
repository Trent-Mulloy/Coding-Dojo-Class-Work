package com.mulloy.OrdersAndItems;

import java.util.ArrayList;

public class Order {
	private String name;
	private Boolean ready;
	private ArrayList<Item> items;
	
	public Order() {
		this.name = "Guest";
		this.items = new ArrayList<Item>();
	}
	public Order(String name) {
		this.name = name;
	}

}
